{
  "redirects": [
    {
      "source": "/home",
      "destination": "/",
      "permanent": false
    },
    {
      "source": "/home/:path*",
      "destination": "/",
      "permanent": false
    },
    {
      "source": "/home.html",
      "destination": "/",
      "permanent": false
    }
  ],
  "rewrites": [
    {
      "source": "/home-v2/main-v2-app/:path*",
      "destination": "/home-v2/main-v2-app/:path*"
    },
    {
      "source": "/:path*",
      "destination": "/index.html"
    }
  ]
}